    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: ht gui",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/ht_gui_controller.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: current file",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: position monitor",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/position_monitor.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: zero position",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/test_home_to_zero.py",
            "console": "integratedTerminal"
        }

    ]
}